{
  "name": "@lagon/astro",
  "version": "0.0.4",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./server.js": "./dist/server.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "watch": "tsup src/index.ts src/server.ts --format=esm --external astro  --watch",
